

<div data-bind="foreach: enquiryModel.car()">
    <div class="col-sm-12">
        <div>
            <strong>Make:</strong><span data-bind="text: $data.car.Make"> </span>
        </div>
        <div>
            <strong>Model:</strong><span data-bind="text: $data.car.Model"> </span>
        </div>
        <div data-bind="if: car.EgcPrice">
            <strong>Exclude Government Charge:</strong><span data-bind="text: ('$' + $data.car.EgcPrice.toFixed(2))"> </span>
        </div>
        <div data-bind="if: car.DapPrice">
            <strong>Drive away price:</strong><span data-bind="text:('$' + $data.car.DapPrice.toFixed(2))"> </span>
        </div>
        <div data-bind="if: car.ContactName">
            <strong>Contact Name:</strong><span data-bind="text: $data.car.ContactName"> </span>
        </div>
        <div data-bind="if: car.Phone">
            <strong>Phone:</strong><span data-bind="text: $data.car.Phone"> </span>
        </div>
        <div>
            <strong>Email:</strong><span data-bind="text: $data.car.Email"> </span>
        </div>
        <div>
            <strong>Contact Name:</strong><span data-bind="text: $data.car.Email"> </span>
        </div>
        <div data-bind="if: car.ABN">
            <strong>ABN:</strong><span data-bind="text: $data.car.Abn"> </span>
        </div>
        <div>
            <strong>Comments:</strong><span data-bind="text: $data.car.Comments"> </span>
        </div>
    </div>
    <div id="contactForm">
        <h2>Submit your enquiry by filling in the required fields</h2>
        <div id='contactsList'>
            <table class='contactsEditor'>
                <tr>
                    <th>First name</th>
                    <th>Last name</th>
                    <th>Email</th>
                </tr>
                <tbody data-bind="foreach: contacts">
                    <tr>
                        <td>
                            * <input id="firstname" data-bind="value: firstName" />
                        </td>
                        <td>* <input id="lastname" data-bind="value: lastName" /></td>
                        <td>
                        <td>* <input id="email" data-bind="value: email" /></td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
    <div class="col-xs-12">
        <div class="col-xs-3"></div>
        <div class="col-xs-3">
            <div class="text-left">
                <button class="btn btn-primary"
                        data-bind="click: sendEnquiry">
                    Send enquiry now
                </button>
            </div>
        </div>
        <div class="col-xs-3">
            <div class="text-right">
                <button class="btn btn-primary"
                        data-bind="click: setView.bind($data, 'list')">
                    Go back
                </button>
            </div>

        </div>
        <div class="col-xs-3"></div>


    </div>
</div>
